generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String
  password String
  Saves    Saves[]
}

model Saves {
  id           Int    @id @default(autoincrement())
  userId       Int
  name         String
  originalName String
  path         String
  originalPath String?
  user         User   @relation(fields: [userId], references: [id])
}
